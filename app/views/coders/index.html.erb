<h2>Listing developers from Africa</h2>
<p>count(<%= @users.count %>)</p>
<table>
  <thead>
    <tr>
      <th>Id</th>
      <th colspan="3">Name</th>
    </tr>
  </thead>

  <tbody>
    <% @directory.each do |user| %>
      <tr>
        <td><%= user[:id] %></td>
        <td><%= user[:login] %></td>
        <td><a href=<%= user[:html_url] %>>Github Link</a></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @directory %>